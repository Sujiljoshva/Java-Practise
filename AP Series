import java.lang.*;
import java.util.*;

public class APSeries{
    public static void main(String args[]){
        Scanner sc =new Scanner(System.in);
        
        System.out.println("Program to print AP Series ");
        System.out.println("Enter value of a,d and n ");
        
        int a= sc.nextInt();
        int d= sc.nextInt();
        int n= sc.nextInt();
        
        int term=a;
        for(int i=0;i<n;i++){
            System.out.println(term);
            term=term+d;
        }
    }
}
