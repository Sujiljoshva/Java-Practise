import java.lang.*;
import java.util.*;

class SumOfNumbers {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter a number to obatian its sum:");
    int n=sc.nextInt();
     int sum=0;
    
    for(int i =1;i<=n;i++)
    {
        sum =sum+i;
        System.out.println(sum);
    
    }
    System.out.println("The sum of is "+sum);
}
}    
