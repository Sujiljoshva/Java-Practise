import java.lang.*;
import java.util.*;

public class Pattern{
    public static void main(String args[]){
        int count =0;
     for(int i=1;i<=5;i++){
         for(int j=1;j<=5-i+1;j++){
             count++;
             System.out.format(j+" ");
         }
         System.out.println("");
     }
        
       
    }
}
