import java.lang.*;
import java.util.*;

class CreatingTable {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    System.out.println("Enter a number to obatian its table:");
    int n=sc.nextInt();
    
    for(int i =1;i<=10;i++)
    {
    System.out.println(i+"x"+n+"="+n*i);
    }
}
}    
